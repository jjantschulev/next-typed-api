  "devDependencies": {
    "@swc/cli": "^0.1.60",
    "@swc/wasm": "^1.3.54",
    "@whop-sdk/turbo-module": "0.0.4",
    "concurrently": "^7.6.0",
    "next": "13.4.1",
    "tsconfig": "workspace:^0.0.0",
    "react-query": "^3.39.3",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react-query": "^3.39.3",
    "next": "^13.4.1"
  },
  "dependencies": {
    "@swc/core": "^1.3.32",
    "chokidar": "^3.5.3",
    "zod": "^3.21.4"
